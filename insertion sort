#include<stdio.h>
void Insertion_sort(int* arr,int len)//插入排序，把数组元素按从小到大排序
{
	int i,j,tmp;
	for (i = 0; i < len-1; i++)
	{
		tmp = arr[i+1];
		for(j=i;j >= 0&&arr[j]>tmp; j--)
		{
				arr[j+1] = arr[j];
		}
		arr[j+1] = tmp;
	}
}
void Printf_arr(int* arr,int len)
{
	int i;
	for (i = 0; i < len; i++)
	{
		printf("%d ",arr[i]);
	}
}
int main()
{
	int arr[10] = {2,3,5,3,1,6,8,9,10,1};
	Insertion_sort(arr,sizeof(arr)/4);
	Printf_arr(arr,sizeof(arr)/4);
	return 0;
}
