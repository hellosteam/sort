#include<stdio.h>
void selection(int* arr,int len)//选择排序，把数组元素按从小到大排序
{
	int i, j;
	for (i = 0; i < len-1; i++)
	{
		for (j =i+1;j < len;j++)
		{
			if (arr[i] > arr[j])
			{
				int tmp = arr[i];
				arr[i] = arr[j];
				arr[j] = tmp;
			}
		}
	}
}
void Printf_arr(int* arr,int len)
{
	int i;
	for (i = 0; i < len; i++)
	{
		printf("%d ",arr[i]);
	}
}
int main()
{
	int arr[10] = {2,3,5,3,1,6,8,9,10,1};
	selection(arr,sizeof(arr)/4);
	Printf_arr(arr,sizeof(arr)/4);
	return 0;
}
